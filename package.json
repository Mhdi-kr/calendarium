{
  "name": "calendrier",
  "version": "0.0.1",
  "description": "Feature-rich, style-less, purely functional and statically typed calendar component for Vue 3",
  "type": "module",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "Mahdi Karimi <mhdi_kr@outlook.com>",
  "main": "./dist/calendrier.umd.cjs",
  "module": "./dist/calendrier.js",
  "exports": {
    ".": {
      "import": "./dist/calendrier.js",
      "require": "./dist/calendrier.umd.cjs"
    }
  },
  "packageManager": "pnpm@7.14.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@types/node": "^18.13.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitepress": "1.0.0-alpha.45",
    "vue": "^3.2.47",
    "vue-tsc": "^1.0.24"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
